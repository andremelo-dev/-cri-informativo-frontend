{
  "name": "informativo",
  "appId": "com.informativo.crimacapa",
  "author": "Andre Melo",
  "description": "Sistema de informação institucional do Cartório Eloy Nunes",
  "main": "public/electron.js",
  "homepage": "./",
  "build": {
    "appId": "com.informativo.crimacapa",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "directories": {
      "buildResources": "build"
    }
  },
  "version": "1.2.0",
  "private": true,
  "dependencies": {
    "@adonisjs/websocket-client": "^1.0.9",
    "@fortawesome/fontawesome-free": "^5.12.0",
    "@material-ui/core": "^4.9.0",
    "@material-ui/icons": "^4.5.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.19.1",
    "bootstrap": "4.4.1",
    "dotenv": "^8.2.0",
    "electron-is-dev": "^1.1.0",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "mui-datatables": "^2.14.0",
    "react": "^16.12.0",
    "react-currency-input": "^1.3.6",
    "react-dom": "^16.12.0",
    "react-input-mask": "^2.0.4",
    "react-lightgallery": "^0.6.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0"
  },
  "devDependencies": {
    "concurrently": "^5.0.2",
    "electron": "^9.4.0",
    "electron-builder": "^21.2.0",
    "wait-on": "^3.3.0"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "build": "npm run react-build && npm run electron-build",
    "start": "concurrently \"SET BROWSER=none&&npm run react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "build --em.main=build/electron.js",
    "preelectron-pack": "npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
